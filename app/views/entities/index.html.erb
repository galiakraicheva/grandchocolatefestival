<p id="notice"><%= notice %></p>

<h1>Entities</h1>

<%= link_to 'New Entity', new_entity_path %>

<table>
  <thead>
    <tr>
      <th>Common name</th>
      <th>Website</th>
      <th>Fb page</th>
      <th>Description</th>
      <th>Progress</th>
      <th>Is Participant?</th>
      <th>Is Sponsor?</th>
      <th>Is Media?</th>
      <th>Is Partner?</th>
      <th>Comments</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @entities.each do |entity| %>
      <tr>
        <td><%= entity.common_name %></td>
        <td><%= entity.website %></td>
        <td><%= entity.fb_page %></td>
        <td><%= entity.description %></td>
        <td><%= entity.progress %></td>
        <td><%= entity.is_participant %></td>
        <td><%= entity.is_sponsor %></td>
        <td><%= entity.is_media %></td>
        <td><%= entity.is_partner %></td>
        <td><%= entity.comments %></td>
        <td><%= entity.country %></td>
        <td><%= link_to 'Show', entity %></td>
        <td><%= link_to 'Edit', edit_entity_path(entity) %></td>
        <td><%= link_to 'Destroy', entity, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Entity', new_entity_path %>
